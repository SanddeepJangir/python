class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        n = len(s)
        max_len = 0
        for i in range(n):
            d = set()
            for j in range(i, n):
                if s[j] in d:
                    break
                d.add(s[j])
            max_len = max(max_len, j - i + 1 )
        return max_len
