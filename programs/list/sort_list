# sort list: default ascending order
a = [4,8,6, 9, 8]
a.sort()
print(a)

# sort the list in descending order
a = [4,56,7,78,42,2]
a.sort(reverse= True)
print(a)

# reverse order: reverse the order the list
a = [2,9,8,7,8,9]
a.reverse()
print(a)

# Sort a list in ascending order without using sort().
li = [5, 2, 4, 10, 3]
for i in range(len(li)):
  for j in range(i + 1, len(li)):
    if li[i] > li[j]:
      li[i], li[j] = li[j], li[i]
print(li)